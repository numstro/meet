# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Email Configuration
# For calendar invites - AWS SES (Required for calendar invite feature)
SES_SMTP_HOST=email-smtp.us-east-1.amazonaws.com
SES_SMTP_PORT=587
SES_SMTP_SECURE=false
SES_SMTP_USER=AKIA...  # Your AWS SES SMTP username
SES_SMTP_PASS=your_ses_smtp_password  # Your AWS SES SMTP password
SES_REGION=us-east-1  # Your AWS region

# For other emails - Resend (Optional - for notifications)
RESEND_API_KEY=your_resend_api_key

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Google Analytics (Optional - for tracking)
NEXT_PUBLIC_GA_MEASUREMENT_ID=your_google_analytics_measurement_id

